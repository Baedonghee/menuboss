  "scripts": {
    "dev": "cross-env BUILD_ENV=local next dev",
    "build": "next build",
    "build:dev": "cross-env BUILD_ENV=development next build",
    "start:dev": "cross-env BUILD_ENV=development pm2 startOrRestart pm2-development-config.json --env development",
    "build:prod": "cross-env BUILD_ENV=production next build",
    "start:prod": "cross-env BUILD_ENV=production pm2-runtime start pm2-config.json --env production",
    "start": "next start",
    "lint": "next lint",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "test": "yarn lint && yarn prettier:check",
    "storybook:dev": "storybook dev -p 6006",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "serve-storybook": "serve storybook-static"
  },
  "git": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
